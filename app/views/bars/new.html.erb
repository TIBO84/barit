<h1>Bars#new</h1>

<%= simple_form_for @bar do |f| %>
    <%= f.input :name %>
    <%= f.input :capacity, as: :numeric %>
    <%= f.input :ambiance %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :price %>
    <%= f.input :description, as: :text %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit %>
<% end %>
